@{
    ViewData["Title"] = "نتیجه ی پرداخت";
}
@section MetaTag
{
    <meta name=googlebot content=noindex,nofollow>
}
<div id="container">
    <div class="container">
        <!-- Breadcrumb Start-->
        <ul class="breadcrumb">
            <li><a asp-area="" asp-controller="Home" asp-action="Index"><i class="fa fa-home"></i></a></li>
            <li><a asp-controller="Account" asp-action="LogOut">خروج</a></li>
        </ul>
        <!-- Breadcrumb End-->
        @if (ViewBag.IsSuccess == true)
        {
            <div class="alert alert-success">
                <i class="fa fa-check-circle"></i>پرداخت با موفقیت انجام شد
                <br />
                <p>
                    کد پیگیری : @ViewBag.Code
                </p>
            </div>
        }
        else
        {
            <div class="alert alert-danger">
                <i class="fa fa-exclamation-circle"></i>پرداخت با شکست مواجه شد
            </div>
        }
    </div>
    
</div>




